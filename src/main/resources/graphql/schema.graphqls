type Player{
    id: ID!,
    name: String,
    team: Team
}

enum Team {
    NAV,
    LEO,
    AGZ,
    TIB,
    LAR
}

type Query{
    findAll: [Player]
    findOne(id: ID): Player

}

type Mutation{
    create(name: String, team: Team): Player
    update(id: ID, name: String, team: Team): Player
    deleteOne(id: ID): Player


}